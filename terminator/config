[global_config]
  title_use_system_font = False
  title_font = MesloLGS NF 11
[keybindings]
  new_tab = <Primary>t
  go_up = <Alt>k
  go_down = <Alt>j
  go_left = <Alt>h
  go_right = <Alt>l
  split_horiz = <Super>h
  split_vert = <Super>v
  close_term = <Primary>q
  next_tab = <Primary>n
  prev_tab = <Primary>p
  layout_launcher = <Primary><Shift><Alt>l
[profiles]
  [[default]]
    cursor_color = "#aaaaaa"
    font = MesloLGS NF 10
    use_system_font = False
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
[plugins]
